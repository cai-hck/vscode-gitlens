{
    "compilerOptions": {
        "lib": [
            "es6",
            "es2015"
        ],
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": false,
        "noUnusedLocals": true,
        "outDir": "out",
        "removeComments": true,
        "rootDir": ".",
        "sourceMap": true,
        "strictNullChecks": false,
        "target": "es6",
        "typeRoots": [
        "./node_modules/@types",
        "./@types"
        ]
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}